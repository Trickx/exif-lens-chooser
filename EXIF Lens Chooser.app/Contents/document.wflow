				<key>ActionBundlePath</key>
				<string>/System/Library/Automator/Run Shell Script.action</string>
				<key>ActionName</key>
				<string>Run Shell Script</string>
				<key>ActionNameComment</key>
				<string>modify EXIF to files based using exiftool</string>
				<key>ActionParameters</key>
				<dict>
					<key>COMMAND_STRING</key>
					<string>
lens_name="$1"
shift

lens_params=""

case "$lens_name" in 
   1.*)
      lens_info='Nikkor 28 mm f/2.8 AIS'
      lens_params="\
              -AFAperture='2.8' \
              -DNGLensInfo='$lens_info' \
              -EffectiveMaxAperture='2.8' \
              -FocalLength='28' \
              -FocalLengthIn35mmFormat='42' \
              -Lens='$lens_info' \
              -LensFStops='6.00' \
              -LensInfo='$lens_info' \
              -LensModel='$lens_info' \
              -LensType='MF' \
              -MaxApertureAtMaxFocal='2.8' \
              -MaxApertureAtMinFocal='2.8' \
              -MaxApertureValue='2.8' \
              -MaxFocalLength='28' \
              -MinFocalLength='28' \
               "
   ;;

   2.*)
   lens_info='Vivitar 28 mm f/2.0 MC Close Focus'
   lens_params="\
           -AFAperture='2.0' \
           -DNGLensInfo='$lens_info' \
           -EffectiveMaxAperture='2.0' \
           -FocalLength='28' \
           -FocalLengthIn35mmFormat='42' \
           -Lens='$lens_info' \
           -LensFStops='6.00' \
           -LensInfo='$lens_info' \
           -LensModel='$lens_info' \
           -LensType='MF' \
           -MaxApertureAtMaxFocal='2.0' \
           -MaxApertureAtMinFocal='2.0' \
           -MaxApertureValue='2.0' \
           -MaxFocalLength='28' \
           -MinFocalLength='28' \
            "
   ;;

   3.*)
   lens_info='Nikon 50 mm f/1.8 Series E'
   lens_params="\
